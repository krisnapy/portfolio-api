{
  "name": "strapi-portfolio",
  "private": true,
  "version": "0.1.0",
  "description": "A Strapi application",
  "scripts": {
    "dev": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "prepare": "husky install",
    "lint": "npx eslint src",
    "lint:fix": "yarn lint src --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "yarn prettier src --write",
    "format": "yarn prettier:fix && yarn lint:fix"
  },
  "lint-staged": {
    "src/**/*.{html,ts,scss}": [
      "prettier --write"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@strapi/plugin-i18n": "4.11.4",
    "@strapi/plugin-users-permissions": "4.11.4",
    "@strapi/provider-upload-cloudinary": "^4.11.4",
    "@strapi/strapi": "4.11.4",
    "better-sqlite3": "8.0.1",
    "pg": "^8.10.0",
    "qs": "^6.11.1"
  },
  "author": "A Strapi developer",
  "strapi": {
    "uuid": "5e71f7ad-7beb-4b71-b1c7-62062d59d144"
  },
  "engines": {
    "node": ">=14.19.1 <=18.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8"
  },
  "main": "index.js",
  "repository": "git@gitlab.com:krisnapy/porfolio-api.git"
}
